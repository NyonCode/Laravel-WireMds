{
    "name": "nyoncode/wire-mds",
    "description": "Framework for automatic registration and management of components in Laravel and Livewire.",
    "authors": [
        {
            "name": "Ondřej Nyklíček",
            "email": "ondrej@nyoncode.cz",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "nyoncode/laravel-package-toolkit": "^1.2.1",
	    "livewire/livewire": "^v3.0",
	    "illuminate/console": "^10|^11|^12",
	    "ext-simplexml": "*",
	    "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "NyonCode\\WireMds\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NyonCode\\WireMds\\Providers\\WireMdsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
